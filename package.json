    "nuxt": "^2.0.0",
    "vuelidate": "^0.7.5"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "pug": "^3.0.1",
    "pug-plain-loader": "^1.0.0",
    "push-dir": "^0.4.1",
    "sass-loader": "^8.0.2"
